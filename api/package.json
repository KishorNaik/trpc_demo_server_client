{
	"name": "NodeExpressApi",
	"version": "1.0.0",
	"description": "Express API Server",
	"author": "",
	"license": "ISC",
	"engines": {
		"node": ">= 14 <= 22"
	},
	"scripts": {
		"start": "cross-env NODE_ENV=production node dist/server.js",
		"start:build": "npm run clean && npx tsc && tsc-alias && swc src -d dist --source-maps --copy-files",
		"start:dev": "cross-env NODE_ENV=development node dist/server.js",
		"start:local": "npm run build && cross-env NODE_ENV=development node dist/server.js",
		"dev": "cross-env NODE_ENV=development nodemon --exec 'node --inspect=0.0.0.0:9229 -r ts-node/register -r tsconfig-paths/register' src/server.ts",
		"build:dev": "npm run clean && npm run prettier && swc src -d dist --source-maps --copy-files",
		"build": "npm run clean && npm run prettier && npm run build:tsc && swc src -d dist --source-maps --copy-files",
		"build:tsc": "npm run prettier && tsc && tsc-alias",
		"test": "glob -c \"node --trace-deprecation --no-warnings --test --require ts-node/register -r tsconfig-paths/register\" \"./src/test/**/*.test.ts\"",
		"run:test": "./run-tests.sh",
		"lint": "eslint --ignore-path .gitignore --ext .ts src",
		"lint:fix": "npm run lint -- --fix",
		"deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
		"deploy:dev": "pm2 start ecosystem.config.js --only dev",
		"prettier": "npx prettier --config .prettierrc --write .",
		"clean": "rimraf dist",
		"prepare": "husky",
		"npm:outdated": "npm outdated",
		"npm:latest-package-update": "npx npm-check-updates -u",
		"trpc:type": "npx dts-bundle-generator ./src/modules/app.Module.ts -o trpc-types/TRPCAppRouter.d.ts"
	},
	"dependencies": {
		"@trpc/client": "^11.4.3",
		"@trpc/server": "^11.4.3",
		"@types/geoip-lite": "^1.4.4",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.2",
		"class-validator-jsonschema": "^5.0.2",
		"cls-hooked": "^4.2.2",
		"compression": "^1.8.0",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^16.5.0",
		"envalid": "^8.0.0",
		"express": "^4.21.2",
		"express-actuator": "^1.8.4",
		"express-jwt": "^8.5.1",
		"express-rate-limit": "^7.5.0",
		"express-slow-down": "^2.1.0",
		"geoip-lite": "^1.4.10",
		"helmet": "^8.1.0",
		"hpp": "^0.2.3",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"reflect-metadata": "^0.2.2",
		"routing-controllers": "^0.11.2",
		"routing-controllers-openapi": "^5.0.0",
		"swagger-ui-express": "^5.0.1",
		"winston": "^3.17.0",
		"winston-daily-rotate-file": "^5.0.0",
		"zod": "^3.25.71"
	},
	"devDependencies": {
		"@faker-js/faker": "^9.8.0",
		"@swc/cli": "^0.7.7",
		"@swc/core": "^1.11.29",
		"@types/autocannon": "^7.12.7",
		"@types/bcrypt": "^5.0.2",
		"@types/compression": "^1.8.0",
		"@types/cookie-parser": "^1.4.8",
		"@types/cors": "^2.8.18",
		"@types/express": "^5.0.0",
		"@types/express-jwt": "^7.4.4",
		"@types/glob": "^8.1.0",
		"@types/hpp": "^0.2.6",
		"@types/ioredis": "^4.28.10",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/morgan": "^1.9.9",
		"@types/node": "^22.15.21",
		"@types/sinon": "^17.0.4",
		"@types/supertest": "^6.0.3",
		"@types/swagger-ui-express": "^4.1.8",
		"@typescript-eslint/eslint-plugin": "^8.32.1",
		"@typescript-eslint/parser": "^8.32.1",
		"autocannon": "^8.0.0",
		"cross-env": "^7.0.3",
		"dts-bundle-generator": "^9.5.1",
		"eslint": "^9.27.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.4.0",
		"expect": "^29.7.0",
		"glob": "^11.0.2",
		"husky": "^9.1.7",
		"lint-staged": "^16.0.0",
		"node-config": "^0.0.2",
		"node-gyp": "^11.2.0",
		"nodemon": "^3.1.10",
		"pm2": "^6.0.6",
		"prettier": "^3.5.3",
		"rimraf": "^6.0.1",
		"sinon": "^20.0.0",
		"supertest": "^7.1.1",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.16",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.8.3"
	}
}
